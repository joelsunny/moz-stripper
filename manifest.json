{
    "manifest_version": 2,
    "name": "stripper",
    "version": "1.0",
  
    "description": "Adds a red border to all webpages matching medium.com",
  
    "icons": {
      "48": "icons/border-48.png"
    },
  
    "content_scripts": [
      {
        "matches": ["*://*.medium.freecodecamp.org/*", "*://*.medium.com/*", "*://*.codeburst.io/*"],
        "js": ["script.js"]
      }
    ]
  
  }